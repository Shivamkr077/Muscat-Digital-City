  function startchat() {
    var fName = $(fName_chat).val().trim();
    var lName = $(lName_chat).val().trim();

    if (fName && lName && fName != '' && lName != '') {
      const UID = fName + lName;

      localStorage.setItem('UUID', UID);

      const user = new CometChatWidget.CometChat.User(UID);
      user.setName(fName + ' ' + lName);
      CometChatWidget.createOrUpdateUser(user).then((user) => {

        // Proceed with user login
        loginCometUserStart(UID);
        $(chat_modal).modal("hide");


      });
    }
